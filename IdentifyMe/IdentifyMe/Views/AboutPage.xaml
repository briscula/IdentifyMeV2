<?xml version="1.0" encoding="utf-8"?>
<mvvm:BaseContentPage x:TypeArguments="viewModels:AboutViewModel" xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:mvvm="clr-namespace:IdentifyMe.MVVM;assembly=IdentifyMe"
             xmlns:viewModels="clr-namespace:IdentifyMe.ViewModels;assembly=IdentifyMe"
             Title="About"
             x:Class="IdentifyMe.Views.AboutPage">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="{Binding Name}"
                   VerticalOptions="CenterAndExpand"
                   HorizontalOptions="CenterAndExpand" />
            
        </StackLayout>
    </ContentPage.Content>
</mvvm:BaseContentPage>